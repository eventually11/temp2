# Clone the repository
git clone https://github.com/eventually1/temp.git
cd temp

# Pull the latest code
git pull origin main

# Push code (Need token)
git remote set-url origin http
git push origin main

# Create and switch branch
git branch
git branch Ferr

# Work on the master (main) branch
git add .
git commit -m "change"
git checkout master

# Merge branch to master (main)
git merge feature-branch

# Make changes
echo "new change" >> README.md
